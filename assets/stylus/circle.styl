.circle
  border-radius (110px)
  width 110px
  height 110px
  border 2px solid $first-color

  &--white
    width 38px
    height 38px
    background $white
    line-height 44px
    text-align center

  &--social
    width 42px
    height 42px
    border 1px solid $white
    background $first-color
    float right
    margin-left 10px